                                <i class="fa fa-minus-square-o"></i>
                                <span class="InvisibleText">[% Translate("Remove Ticket Customer User") | html %]</span>
                            </a>
                        </div>

                        <div id="TicketCustomerContentFromCustomer" class="CustomerContainer CustomerContainerAgentTicketRequestOFORK">
[% RenderBlockStart("MultipleCustomer") %]
