<script type="text/javascript">//<![CDATA[

    $('#[% Data.FeldName | html %]').bind('change', function () {
        var SelectedID     = $("#[% Data.FeldName | html %]").val();
        var NewBlockValues = $("#BlockValues").val();
        var NewLink;
        if ( SelectedID ) {

		    var SessionName = $('input#SessionName').val();
		    var SessionID = $('input#SessionID').val();
		    var new_names = SessionName + "=" + SessionID;

            $.ajax({
                type:     'Post',
                url:      'index.pl?' + new_names,
                data:
                {
                    Action:            'AgentTicketRequestBlock',
                    Subaction:         'ShowBlock',
                    RequestID:          [% Data.RequestID | html %],
                    RequestFormID:      [% Data.FeldAktuellID | html %],
                    RequestFormValueID: SelectedID,
                    NewBlockValues:    NewBlockValues,
                },
                async: false,
                dataType: 'html',
                success: function(Response) {
                    $('#[% Data.FeldName | html %]BlockHTML').html(Response);
                }
            });
        }
        else {
            $('#[% Data.FeldName | html %]BlockHTML').html('');
        }
    });

    $('#[% Data.FeldName | html %]').each(function () {
        var SelectedID     = $("#[% Data.FeldName | html %]").val();
