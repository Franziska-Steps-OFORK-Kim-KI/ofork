<input type="hidden" name="[% Data.JSONFieldName | html %]" class="StatsSettingsJSON" />
<div class="StatsSettingsBox" id="StatsSettingsBox[% Data.NamePref | html %]">

[% FOR Error IN Data.Errors %]
