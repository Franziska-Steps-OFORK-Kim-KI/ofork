<div id="CompanyInformation" class="OFORKCompanyInformationDiv"></div>

<div id="Footer" class="ARIARoleContentinfo">

[% IF(Data.FooterLinks) %]
    <ul class="FooterLinks">
        [% FOREACH Link IN Data.FooterLinks %]
            <li>
                <a href="[% Link.Target %]" target="_blank">[% Link.Description | html %]</a>
            </li>
        [% END %]
    </ul>
[% END %]

<div class="OFORKFooter1"></div>
<div class="OFORKFooter2"></div>
<div class="OFORKFooter3"></div>
<div class="OFORKFooter4"></div>

[% IF !Config("Secure::DisableBanner") %]
    <a href="https://o-fork.de target="_blank">
        [% Translate("Powered by %sâ„¢", "OFORK") | html %]
    </a>
    <div class="Clear"></div>
[% END %]


</div>

<div class="OFORKFooter5"></div>

[% InsertTemplate("CustomerFooterJS.tt") %]

</body>
</html>
